{
  "name": "rk-designsystem",
  "version": "1.1.75",
  "description": "A React component library built on top of Digdir Design System",
  "author": "daniel@tunetek.no",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "typescript",
    "digdir",
    "norwegian"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/norwegianredcross/DesignSystem.git"
  },
  "homepage": "https://norwegianredcross.github.io/DesignSystem/storybook",
  "bugs": {
    "url": "https://github.com/norwegianredcross/DesignSystem/issues"
  },
  "private": false,
  "type": "module",
  "main": "./dist/componentlibrary.umd.js",
  "module": "./dist/componentlibrary.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/componentlibrary.es.js",
      "require": "./dist/componentlibrary.umd.js"
    },
    "./dist/componentlibrary.css": "./dist/componentlibrary.css",
    "./dist/rk-designsystem.css": "./dist/rk-designsystem.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "@digdir/designsystemet": "^1.9.0",
    "@digdir/designsystemet-css": "^1.9.0",
    "@digdir/designsystemet-react": "^1.9.0",
    "@navikt/aksel-icons": "^7.20.0",
    "date-fns": "^4.1.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rimraf dist",
    "changeset": "changeset",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run clean && npm run build",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run generate-metadata && npm run update-ai-guide-components && storybook build --output-dir storybook-build",
    "generate-metadata": "node generate-metadata.mjs",
    "update-ai-guide-components": "node update-ai-guide-components.mjs",
    "predeploy:storybook": "npm run build-storybook && npm run copy-metadata && npm run copy-ai-guide",
    "copy-metadata": "node -e \"require('fs').copyFileSync('metadata.json', 'storybook-build/metadata.json')\"",
    "copy-ai-guide": "node -e \"require('fs').copyFileSync('AI_DESIGN_SYSTEM_GUIDE.md', 'storybook-build/AI_DESIGN_SYSTEM_GUIDE.md')\"",
    "deploy:storybook": "gh-pages -d storybook-build -b gh-pages -r https://github.com/norwegianredcross/DesignSystem.git --dest storybook",
    "build:app": "vite build --config vite.app.config.ts",
    "deploy:app": "npm run build:app && gh-pages -d dist-app -b gh-pages -r https://github.com/norwegianredcross/DesignSystem.git --add"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@chromatic-com/storybook": "^4.1.2",
    "@digdir/designsystemet": "^1.0.3",
    "@digdir/designsystemet-css": "^1.0.3",
    "@digdir/designsystemet-react": "^1.0.3",
    "@eslint/js": "^9.21.0",
    "@navikt/aksel-icons": "^7.20.0",
    "@storybook/addon-a11y": "^10.0.4",
    "@storybook/addon-docs": "^10.0.4",
    "@storybook/addon-links": "^10.0.4",
    "@storybook/addon-onboarding": "^10.0.4",
    "@storybook/react-vite": "^10.0.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^10.0.4",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.3",
    "globals": "^15.15.0",
    "playwright": "^1.52.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-docgen-typescript": "^2.4.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "storybook": "^10.0.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@digdir/designsystemet-theme": "^1.6.0",
    "@types/classnames": "^2.3.0",
    "classnames": "^2.5.1",
    "embla-carousel-react": "^8.6.0",
    "rk-design-tokens": "^1.0.12"
  }
}
